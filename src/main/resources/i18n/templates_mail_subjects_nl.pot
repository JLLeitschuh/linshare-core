# Subject ANONYMOUS_DOWNLOAD
# LinShare: An anonymous user downloaded the file you shared
msgid "mail_subjects_0"
msgstr "LinShare : Een anonieme gebruiker heeft het door u gedeelde bestand gedownload"

# Subject REGISTERED_DOWNLOAD
# LinShare: An user downloaded the file you shared
msgid "mail_subjects_1"
msgstr "LinShare : Een gebruiker heeft het door u gedeelde bestand gedownload"

# Subject NEW_GUEST
# LinShare: Your account on LinShare has been created
msgid "mail_subjects_2"
msgstr "LinShare : Uw LinShare account werd aangemaakt."

# Subject RESET_PASSWORD
# LinShare: Your password was reset
msgid "mail_subjects_3"
msgstr "LinShare : Uw nieuwe wachtwoord"

# Subject NEW_SHARING
# LinShare: A user deposited files in sharing for you
msgid "mail_subjects_4"
msgstr "LinShare : Een gebruiker heeft te delen bestanden voor u klaargezet."

# Subject SHARED_DOC_UPDATED
# LinShare: An user has updated a shared file
msgid "mail_subjects_5"
msgstr "LinShare : Een gebruiker heeft een gedeeld bestand bijgewerkt"

# Subject NEW_GROUP_SHARING
# LinShare: A user deposited files in sharing for the group ${groupName}
msgid "mail_subjects_6"
msgstr "LinShare : Een gebruiker heeft te delen bestanden klaargezet voor de groep ${groupName}"

# Subject MEMBERSHIP_REQUEST_STATUS
# LinShare: Status of your membership request for the group ${groupName} and the user ${newMemberFirstName} ${newMemberLastName}
msgid "mail_subjects_7"
msgstr "LinShare : Status van uw aanvraag tot toetreding tot de groep ${groupName} en de gebruiker ${newMemberFirstName} ${newMemberLastName}"

# Subject NEW_GROUP_MEMBER
# LinShare: You are now member of the group ${groupName}
msgid "mail_subjects_8"
msgstr "LinShare : U bent nu lid van de groep ${groupName}"

# Subject GROUP_SHARING_DELETED
# LinShare: A file shared with the group ${groupName} has been deleted.
msgid "mail_subjects_9"
msgstr "LinShare : Een bestand gedeeld met de groep ${groupName} werd gewist"

# Subject SHARED_DOC_DELETED
# LinShare: An user has deleted a shared file
msgid "mail_subjects_10"
msgstr "LinShare : Een gebruiker heeft een gedeeld bestand gewist"

# Subject SHARED_DOC_UPCOMING_OUTDATED
# LinShare: A sharing will be soon deleted
msgid "mail_subjects_11"
msgstr "LinShare : Een share zal binnenkort gewist worden."
