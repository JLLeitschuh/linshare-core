<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.linagora.linShare.core.domain.entities.Domain" table="DOMAIN">
    <id column="domain_id" name="persistenceId">
      <generator class="native"/>
    </id>
    <property access="field" column="identifier" index="index_domain_identifier" name="identifier" not-null="true" unique="true"/>
    <property access="field" column="differential_key" name="differentialKey"/>
    <many-to-one lazy="false" access="field" class="org.linagora.linShare.core.domain.entities.DomainPattern" column="domain_pattern_id_fk" name="pattern" not-null="true"/>
    <many-to-one lazy="false" access="field" class="org.linagora.linShare.core.domain.entities.LDAPConnection" column="ldap_connection_id_fk" name="ldapConnection" not-null="true"/>
  </class>
</hibernate-mapping>
