<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.linagora.linShare.core.domain.entities.DomainPattern" table="DOMAIN_PATTERN">
    <id column="domain_pattern_id" name="persistenceId">
      <generator class="native"/>
    </id>
    <property access="field" column="identifier" index="index_domain_pattern_identifier" name="identifier" not-null="true" unique="true"/>
    <property access="field" column="description" name="description"/>
    <property access="field" column="get_user_command" name="getUserCommand"/>
    <property access="field" column="get_all_domain_users_command" name="getAllDomainUsersCommand"/>
    <property access="field" column="is_admin_command" name="isAdminCommand"/>
    <property access="field" column="auth_command" name="authCommand"/>
    <property access="field" column="search_user_command" name="searchUserCommand"/>
    <property access="field" column="get_user_result" name="getUserResult"/>
    <property access="field" column="get_all_domain_users_result" name="getAllDomainUsersResult"/>
    <property access="field" column="is_admin_result" name="isAdminResult"/>
    <property access="field" column="auth_result" name="authResult"/>
    <property access="field" column="search_user_result" name="searchUserResult"/>
  </class>
</hibernate-mapping>
