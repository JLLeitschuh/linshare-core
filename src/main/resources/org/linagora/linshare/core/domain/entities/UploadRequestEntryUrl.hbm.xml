<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="org.linagora.linshare.core.domain.entities.UploadRequestEntryUrl"
		table="upload_request_entry_url" lazy="false">
		<id name="id" column="id" type="long" unsaved-value="0">
			<generator class="native"></generator>
		</id>
		<many-to-one name="uploadRequestEntry" column="upload_request_entry_id"
			class="org.linagora.linshare.core.domain.entities.UploadRequestEntry"
			unique="true" not-null="true" lazy="false"></many-to-one>
		<property name="uuid" column="uuid" type="string" length="255" not-null="true" lazy="false" />
		<property name="path" column="path" type="string" length="255" not-null="true" lazy="false" />
		<property name="password" column="password" type="string" length="255" not-null="false" lazy="false" />
		<property name="creationDate" column="creation_date" type="timestamp" not-null="true" lazy="false" />
		<property name="modificationDate" column="modification_date" type="timestamp" not-null="true" lazy="false" />
		<property name="expiryDate" column="expiry_date" type="timestamp" not-null="true" lazy="false" />
	</class>
</hibernate-mapping>