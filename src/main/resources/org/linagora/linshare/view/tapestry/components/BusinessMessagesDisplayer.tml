<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<t:container xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
    <t:if test="prop:businessErrors" negate="false">
        <div id="errorMessages" class="errorMessagesClass">
            <t:loop t:source="prop:businessErrors" t:value="errorMessage">
                <ul>
                    <li><t:outputraw value="${errorMessage}"/></li>
                </ul>
            </t:loop>
        </div>
    </t:if>
</t:container>
